<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.stock.forecast_service.domain.example.dao.ExampleDao">

    <select id="findByName" resultType="com.stock.forecast_service.domain.example.entity.Example">
        SELECT
            id,
            name,
            create_date
        FROM example
        WHERE name = #{name}
    </select>

</mapper>